			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="kundeDescription" />
			<basic name="userKunde" />
			<basic name="userDocument" />
			<basic name="userCity" />
			<basic name="userEmail" />
			<basic name="folderDLId" />
